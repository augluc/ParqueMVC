@model Brinquedo

<div class="mb-3">
	<label asp-for="Nome"></label>
	<input asp-for="Nome" class="form-control" />
</div>
<div class="mb-3">
	<label asp-for="DataFabricacao"></label>
	<input asp-for="DataFabricacao" class="form-control" />
</div>
<div class="mb-3">
	<label asp-for="Valor"></label>
	<input asp-for="Valor" class="form-control" />
</div>
<div class="mb-3">
	<label asp-for="Tipo"></label>
	<select asp-for="Tipo" asp-items="@Html.GetEnumSelectList<Tipo>()" class="form-control">
		<option>Selecione</option>
	</select>
</div>
<div class="mb-3">
	<label asp-for="Manutencao">Manutenção</label>
	<select asp-for="ManutencaoId" class="form-control" asp-items="@ViewBag.Manutencao">
		<option>Selecione</option>
	</select>

</div>
<div class="mb-3">
	<label asp-for="PrecisaAcompanhante"></label>
	<input asp-for="PrecisaAcompanhante" class="form-control"/>
</div>
