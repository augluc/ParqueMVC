
@{
    ViewData["Title"] = "Cadastrar Brinquedo";
}

<h1>Cadastrar Nova Atração</h1>

<hr />
<msg texto="@TempData["msg"]" class="my-3 alert alert-warning"></msg>

<form asp-action="Cadastrar">

    <partial name="_Form"/>

    <botao texto="Cadastrar"></botao>
    <input type="submit" value="Cancelar" asp-controller="brinquedo" asp-action="index" class="btn btn-danger" />
</form>

