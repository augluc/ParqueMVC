@model Colaborador
@{
    ViewData["Title"] = "Editar Colaborador";
}

<h1>Editar Colaborador</h1>


<form asp-action="Editar">
    <input asp-for="ColaboradorId" />
    <partial name="_Form"/>

    <botao texto="Editar"></botao>

    <input type="submit" value="Cancelar" asp-controller="colaborador" asp-action="index" class="btn btn-danger" />
</form>
